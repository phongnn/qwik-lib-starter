{
  "name": "qwik-lib-starter",
  "version": "0.0.1",
  "description": "A template for creating a new library for Qwik framework.",
  "homepage": "https://github.com/phongnn/qwik-lib-starter",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "qwik-lib-starter": "./dist/index.mjs",
  "scripts": {
    "dev": "node --inspect node_modules/vite/bin/vite.js --config vite.example.ts --mode ssr",
    "build.example": "vite build --config vite.example.ts --ssr src/entry.ssr.tsx",
    "build": "pnpm build.lib && pnpm build.types",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "np",
    "typecheck": "tsc --incremental --noEmit"
  },
  "devDependencies": {
    "@types/eslint": "8.4.6",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.34.0",
    "@typescript-eslint/parser": "5.34.0",
    "eslint": "8.22.0",
    "eslint-plugin-qwik": "0.0.107",
    "node-fetch": "3.2.10",
    "np": "7.6.1",
    "prettier": "2.7.1",
    "typescript": "4.7.4",
    "vite": "3.0.9"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ]
}
